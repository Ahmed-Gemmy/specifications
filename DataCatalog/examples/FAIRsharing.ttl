<fso> a schema:DataCatalog ;
    schema:dataset [ a schema:Dataset ;
            schema:description "A manually curated registry of data policies from research funders, journal publishers, societies, and other organisations. These are linked to the databases and standards that they recommend for use" ;
            schema:identifier <https://www.fairsharing.org/bsg-pXXXXXX> ;
            schema:keywords "Data policy, journal, funder, society" ;
            schema:name "Data Policy" ;
            schema:url <https://fairsharing.org/policies> ],
        [ a schema:Dataset ;
            schema:description "A manually curated registry of standards, split into three types - Terminology Artifacts (ontologies, e.g. Gene Ontology), Models and Formats (conceptual schema, formats, data models, e.g. FASTA), and Reporting Guidelines (e.g. the ARRIVE guidelines for in vivo animal testing). These are linked to the databases that implement them and the funder and journal publisher data policies that recommend or endorse their use." ;
            schema:identifier <https://www.fairsharing.org/bsg-sXXXXXX> ;
            schema:includedInDataCatalog "https://www.fairsharing.org" ;
            schema:keywords "Standards, Metadata, Formats, Ontologies, Terminology Artifacts, Reporting Guidelines" ;
            schema:name "Metadata Standard" ;
            schema:url <https://fairsharing.org/standards> ],
        [ a schema:Dataset ;
            schema:description "A manually curated registry of databases/data repositories, conforming to the BioDBcore standard (from the Life Sciences). These are linked to the standards that they use and the funder and journal publisher data policies that recommend or endorse their use." ;
            schema:identifier <https://www.fairsharing.org/biodbcoreXXXXXX> ;
            schema:includedInDataCatalog "https://www.fairsharing.org" ;
            schema:keywords "Database, Data repository" ;
            schema:name "Database" ;
            schema:url <https://fairsharing.org/databases> ] ;
    schema:description "A manually curated, informative and educational resource on data and metadata standards, inter-related to databases/data repositories and funder and journal publisher data policies from across disciplines. FAIRsharing is an ELIXIR-UK node resource and has an active role in the RDA and Force11 data initiatives." ;
    schema:identifier <https://identifiers.org/MIR:00000364> ;
    schema:keywords "registry, life science, natural science, social science, " ;
    schema:license [ a schema:CreativeWork ;
            schema:name "Creative Commons CC BY-SA 4.0 Attribution" ;
            schema:url <https://creativecommons.org/licenses/by-sa/4.0/> ] ;
    schema:name "FAIRsharing.org" ;
    schema:alternateName "BioSharing.org", "FAIRsharing.org Registry" ; #Not meant to be part of the provider information
    schema:provider [ a schema:Organization ;
            schema:name "ELIXIR-UK" ;
            schema:url <http://www.elixir-uk.org/> ],
        [ a schema:Person ;
            schema:email "biosharing-contact-us@lists.sf.net" ;
            schema:name "FAIRsharing.org support" ],
        [ a schema:Organization ;
            schema:name "Oxford e-Research Centre, University of Oxford" ;
            schema:url <http://www.oerc.ox.ac.uk/> ] ;
    schema:publication [ a schema:PublicationEvent ;
            schema:name "BioSharing: curated and crowd-sourced metadata standards, databases and data policies in the life sciences." ;
            schema:url <https://academic.oup.com/database/article-lookup/doi/10.1093/database/baw075> ] ;
    #Added citation as it is a Recommended property
    schema:citation [ a schema:CreativeWork ;
            schema:name "BioSharing: curated and crowd-sourced metadata standards, databases and data policies in the life sciences." ;
            schema:url <https://academic.oup.com/database/article-lookup/doi/10.1093/database/baw075> ] ;
